{
  "name": "army-list",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "flow": "flow",
    "flow:watch": "watchman-make -p 'App/**/*.js' -p 'components/**/*.js' -p 'quark/**/*.js' --make 'yarn run' -t flow",
    "format": "prettier --write '{,!(node_modules)/**/}*.js'",
    "test": "jest",
    "build:schema": "graphql get-schema graphcool",
    "graphcool": "graphcool",
    "relay": "relay-compiler --src . --schema ./schema.graphql"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": false
  },
  "dependencies": {
    "babel-plugin-root-import": "^5.1.0",
    "expo": "^21.0.0",
    "flow": "^0.2.3",
    "graphcool": "^1.3.7",
    "graphql-cli": "^1.1.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lodash": "^4.17.4",
    "quark-native": "^0.0.9-beta.11",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.48.4",
    "react-native-portal": "^1.2.1",
    "react-relay": "^1.4.1",
    "react-router-native": "^4.2.0"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.4.1",
    "flow-bin": "0.49.1",
    "flow-typed": "^2.1.5",
    "git-hooks": "^1.1.8",
    "jest-expo": "^21.0.2",
    "prettier": "^1.7.3",
    "react-native-scripts": "1.5.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "regenerator-runtime": "^0.11.0",
    "relay-compiler": "^1.4.1",
    "start": "^5.1.0"
  }
}
